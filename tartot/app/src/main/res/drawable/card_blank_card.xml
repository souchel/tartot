<?xml version="1.0" encoding="utf-8"?>

<!-- To fit the imageButton (or the imageView), the vector width must correspond to the viewPort height.
/!\ for the imageView, the dimension (width height ration) of the imageView are important but can be chosen, the vector will fit, BUT
it's not the case for the imageButton and the dimensions (width and height) of the imageButton MUST be the same -->

<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="80px"
    android:height="120px"
    android:viewportWidth="80.0"
    android:viewportHeight="120.0">
        <!-- /!\ pour le path (en particulier le pathData) le point d'originge (X=0,Y=0) est en haut Ã  gauche donc Y est vers le bas ! -->
        <path
            android:name="card"
            android:fillColor="#FFFFFF"
            android:strokeColor="#000000"
            android:strokeWidth="2"
            android:pathData="M00,00 L80,00 L80,120 L00,120 z"/>
</vector>